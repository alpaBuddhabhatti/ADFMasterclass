{
	"name": "LAB21",
	"properties": {
		"description": "Please input a folder path as the parameter for this pipeline, and the pipeline will delete files which were last modified more than 30 days ago under the particular folder path.",
		"activities": [
			{
				"name": "DeleteFiles",
				"description": "Connect to your storage account to save the log file.",
				"type": "Delete",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataset": {
						"referenceName": "DS_ADLG_BIN_DEL_ACT_SINGLE",
						"type": "DatasetReference"
					},
					"enableLogging": false,
					"storeSettings": {
						"type": "AzureBlobFSReadSettings",
						"recursive": true,
						"modifiedDatetimeEnd": {
							"value": "@{adddays(pipeline().TriggerTime, -60)}",
							"type": "Expression"
						},
						"wildcardFileName": "*"
					}
				}
			}
		],
		"folder": {
			"name": "Lab2"
		},
		"annotations": []
	}
}